{
  "log": {
    "access": "/var/log/xray/access.log",
    "error": "/var/log/xray/error.log",
    "loglevel": "info"
  },
  "dns": {
    "hosts": {
      "cloudflare-dns.com": [
        "1.1.1.1",
        "1.0.0.1",
        "2606:4700:4700::1111",
        "2606:4700:4700::1001"
      ],
      "dns.alidns.com": [
        "223.5.5.5",
        "223.6.6.6",
        "2400:3200::1",
        "2400:3200:baba::1"
      ]
    },
    "servers": [
      "https://cloudflare-dns.com/dns-query",
      {
        "address": "localhost",
        "domains": ["ext:geosite.dat:private"],
        "expectIPs": ["ext:geoip.dat:private"]
      },
      {
        "address": "https://cloudflare-dns.com/dns-query",
        "domains": ["ext:geosite.dat:google", "ext:geosite.dat:apple"]
      },
      {
        "address": "https://dns.alidns.com/dns-query",
        "domains": [
          "ext:geosite.dat:tld-cn",
          "ext:geosite.dat:category-games@cn"
        ]
      },
      {
        "address": "https://cloudflare-dns.com/dns-query",
        "domains": ["ext:geosite.dat:geolocation-!cn"],
        "expectIPs": ["ext:geoip.dat:!cn"]
      },
      {
        "address": "https://dns.alidns.com/dns-query",
        "domains": ["ext:geosite.dat:cn"],
        "expectIPs": ["ext:geoip.dat:cn"]
      }
    ],
    "disableFallbackIfMatch": true,
    "tag": "dns"
  },
  "routing": {
    "domainStrategy": "IPIfNonMatch",
    "domainMatcher": "hybrid",
    "rules": [
      {
        "type": "field",
        "inboundTag": ["socks_inbound"],
        "outboundTag": "xhttp_outbound"
      },
      {
        "type": "field",
        "protocol": ["bittorrent"],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "ip": [
          "223.5.5.5",
          "223.6.6.6",
          "2400:3200::1",
          "2400:3200:baba::1",
          "ext:geoip.dat:private"
        ],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "domain": ["ext:geosite.dat:private"],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "ip": [
          "ext:geoip.dat:google",
          "1.1.1.1",
          "1.0.0.1",
          "2606:4700:4700::1111",
          "2606:4700:4700::1001"
        ],
        "outboundTag": "xhttp_outbound"
      },
      {
        "type": "field",
        "domain": ["ext:geosite.dat:google", "ext:geosite.dat:apple"],
        "outboundTag": "xhttp_outbound"
      },
      {
        "type": "field",
        "domain": [
          "ext:geosite.dat:tld-cn",
          "ext:geosite.dat:category-games@cn"
        ],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "domain": ["ext:geosite.dat:geolocation-!cn"],
        "ip": ["ext:geoip.dat:!cn"],
        "outboundTag": "xhttp_outbound"
      },
      {
        "type": "field",
        "domain": ["ext:geosite.dat:cn"],
        "ip": ["ext:geoip.dat:cn"],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "ip": ["ext:geoip.dat:cn"],
        "outboundTag": "direct_outbound"
      },
      {
        "type": "field",
        "network": "tcp,udp",
        "outboundTag": "direct_outbound"
      }
    ]
  },
  "inbounds": [
    {
      "port": "{PORT}",
      "protocol": "socks",
      "tag": "socks_inbound",
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls", "quic"]
      }
    }
  ],
  "outbounds": [
    {
      "tag": "xhttp_outbound",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "{ADDRESS}",
            "port": 443,
            "users": [
              {
                "id": "{ID}",
                "encryption": "none"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "xhttp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "fingerprint": "edge",
          "serverName": "{serverName}",
          "publicKey": "{publicKey}",
          "shortId": "{shortId}",
          "mldsa65Verify": "{mldsa65Verify}"
        },
        "xhttpSettings": {
          "host": "{host}",
          "path": "{path}",
          "mode": "auto"
        }
      }
    },
    {
      "tag": "direct_outbound",
      "protocol": "freedom",
      "settings": {
        "domainStrategy": "UseIP"
      }
    },
    {
      "tag": "block_outbound",
      "protocol": "blackhole",
      "settings": {
        "response": {
          "type": "http"
        }
      }
    }
  ]
}
